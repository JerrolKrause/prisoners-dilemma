{
  "name": "angular-starter",
  "license": "MIT",
  "version": "5.0.1",
  "scripts": {
    "localize": "ren tsconfig.json tsconfig.old.json && ren tsconfig.bak.json tsconfig.json",
    "start": "ng serve --hmr",
    "reinstall": "rmdir node_modules /s /q && rmdir .angular /s /q && del package-lock.json /q &&  npm i",
    "format": "npm run format-glob -- \"src/{app,environments}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format-glob": "prettier --write",
    "ng": "ng",
    "build": "ng build",
    "build:stats": "ng build --configuration=production --stats-json && npm run bundle-report",
    "build:source": "ng build --configuration=production --source-map",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json -p 8887",
    "prod": "http-server ./dist/browser -o -g",
    "prod:static": "http-server ./dist/static -o -g",
    "build:qa": "ng build --configuration=qa",
    "build:uat": "ng build --configuration=uat",
    "build:prod": "ng build --configuration=production",
    "ssr:dev": "ng run angular-starter:serve-ssr",
    "ssr:build": "ng build --configuration=production && ng run angular-starter:server",
    "ssr:serve": "node dist/server/main.js",
    "ssr:prerender": "ng run angular-starter:prerender --routes-file routes.txt",
    "deploy": "ng build --configuration=production --base-href /angular-starter/ && angular-cli-ghpages --no-silent --dir=dist/browser",
    "deploy:ssr": "ng run angular-starter:deploy-ssr --routes-file routes.txt && angular-cli-ghpages --no-silent --dir=dist/browser",
    "gzip": "gzipper --verbose ./dist/browser",
    "docs": "compodoc -p tsconfig.json",
    "docs:serve": "compodoc -p tsconfig.json -s",
    "docs:watch": "compodoc -p tsconfig.json -s -w",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --configuration=production && ng run angular-starter:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build-ie-polyfills": "webpack-cli src/polyfills-ie.ts -o src/assets/scripts/polyfills-ie.min.js -c webpack-polyfill.config.js"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 160,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "17.0.1",
    "@angular/animations": "17.0.3",
    "@angular/cdk": "17.0.1",
    "@angular/common": "17.0.3",
    "@angular/compiler": "17.0.3",
    "@angular/core": "17.0.3",
    "@angular/elements": "17.0.3",
    "@angular/forms": "17.0.3",
    "@angular/platform-browser": "17.0.3",
    "@angular/platform-browser-dynamic": "17.0.3",
    "@angular/platform-server": "17.0.3",
    "@angular/router": "17.0.3",
    "@angular/service-worker": "17.0.3",
    "@angular/ssr": "^17.0.1",
    "@fortawesome/angular-fontawesome": "0.13.0",
    "@fortawesome/fontawesome-free": "6.3.0",
    "@fortawesome/fontawesome-svg-core": "6.3.0",
    "@fortawesome/free-brands-svg-icons": "6.3.0",
    "@fortawesome/free-regular-svg-icons": "6.3.0",
    "@fortawesome/free-solid-svg-icons": "6.3.0",
    "@ntersol/schematics": "1.0.9",
    "@ntersol/services": "1.0.3",
    "@ntersol/state-management": "^4.2.13",
    "@schematics/update": "0.1102.18",
    "bootstrap": "5.3.2",
    "compression": "^1.7.4",
    "core-js": "3.23.1",
    "document-register-element": "1.14.10",
    "express": "^4.18.2",
    "primeicons": "^6.0.1",
    "primeng": "16.4.2",
    "ramda": "0.29.1",
    "rxjs": "7.5.5",
    "sass": "1.69.5",
    "ts-loader": "9.3.0",
    "tslib": "2.4.0",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.1",
    "@angular/cli": "17.0.1",
    "@angular/compiler-cli": "17.0.3",
    "@angular/language-service": "17.0.3",
    "@compodoc/compodoc": "1.1.19",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/gtag.js": "0.0.12",
    "@types/jest": "29.5.1",
    "@types/node": "18.14.5",
    "@types/ramda": "0.28.23",
    "angular-cli-ghpages": "1.0.6",
    "codelyzer": "6.0.2",
    "eslint": "8.35.0",
    "express": "4.18.1",
    "jasmine-marbles": "0.9.2",
    "jest": "29.5.0",
    "jest-preset-angular": "13.1.0",
    "ng-packagr": "17.0.1",
    "prettier": "2.7.0",
    "protractor": "7.0.0",
    "schematics-utilities": "2.0.3",
    "source-map-explorer": "2.5.2",
    "ts-node": "10.8.1",
    "tslint-angular": "3.0.3",
    "typescript": "5.2.2",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.6.1",
    "webpack-cli": "4.10.0"
  }
}
