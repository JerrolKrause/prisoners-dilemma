{
  "name": "angular-starter",
  "license": "MIT",
  "scripts": {
    "localize": "ren tsconfig.json tsconfig.old.json && ren tsconfig.bak.json tsconfig.json",
    "start": "npm run format && ng serve",
    "reinstall": "del package-lock.json /q && rmdir node_modules /s /q && npm i",
    "format": "npm run format-glob -- \"src/{app,environments}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format-glob": "prettier --write",
    "ng": "ng",
    "bit:pull": "bit import && bit checkout latest --all",
    "bit:push": "bit tag --all && bit export nts.angular",
    "bit:reset": "bit checkout --all --reset",
    "build": "ng build",
    "build:stats": "ng build --configuration=production --stats-json && npm run bundle-report",
    "build:source": "ng build --configuration=production --source-map",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json -p 8887",
    "prod": "http-server ./dist/browser -o -g",
    "prod:static": "http-server ./dist/static -o -g",
    "build:qa": "ng build --configuration=qa",
    "build:uat": "ng build --configuration=uat",
    "build:prod": "ng build --configuration=production",
    "build:ssr": "npm run build:prod && npm run scully:scan",
    "scully:build": "npx scully",
    "scully:scan": "npx scully --scanRoutes",
    "scully:watch": "npx scully --watch",
    "scully:serve": "npx scully serve",
    "au:dev": "ng run angular-starter:serve-ssr",
    "au:build": "ng build && ng run angular-starter:server",
    "au:serve": "node dist/server/main.js",
    "au:prerender": "ng run angular-starter:prerender --routes-file routes.txt",
    "deploy": "ng build --prod --base-href /angular-starter/ && angular-cli-ghpages --no-silent --dir=dist/browser",
    "gzip": "gzipper --verbose ./dist/browser",
    "docs": "compodoc -p tsconfig.json",
    "docs:serve": "compodoc -p tsconfig.json -s",
    "docs:watch": "compodoc -p tsconfig.json -s -w",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-starter:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build-ie-polyfills": "webpack-cli src/polyfills-ie.ts -o src/assets/scripts/polyfills-ie.min.js -c webpack-polyfill.config.js"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 160,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "14.0.1",
    "@angular/animations": "14.0.1",
    "@angular/cdk": "14.0.1",
    "@angular/common": "14.0.1",
    "@angular/compiler": "14.0.1",
    "@angular/core": "14.0.1",
    "@angular/elements": "14.0.1",
    "@angular/forms": "14.0.1",
    "@angular/platform-browser": "14.0.1",
    "@angular/platform-browser-dynamic": "14.0.1",
    "@angular/platform-server": "14.0.1",
    "@angular/router": "14.0.1",
    "@angular/service-worker": "14.0.1",
    "@fortawesome/angular-fontawesome": "0.11.0",
    "@fortawesome/fontawesome-free": "6.1.1",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-regular-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@ngneat/until-destroy": "9.2.0",
    "@nguniversal/express-engine": "14.0.0",
    "@ntersol/forms": "0.0.7",
    "@ntersol/schematics": "1.0.7",
    "@ntersol/state-management": "3.0.6",
    "@schematics/update": "0.1102.18",
    "@scullyio/init": "2.1.36",
    "@scullyio/ng-lib": "2.1.36",
    "@scullyio/platform-server": "2.1.36",
    "@scullyio/scully": "2.1.36",
    "@scullyio/scully-plugin-flash-prevention": "2.1.36",
    "@scullyio/scully-plugin-puppeteer": "2.1.36",
    "bootstrap": "5.1.3",
    "core-js": "3.23.1",
    "document-register-element": "1.14.10",
    "express": "4.18.1",
    "helpful-decorators": "2.1.0",
    "primeicons": "5.0.0",
    "primeng": "13.4.1",
    "ramda": "0.28.0",
    "rxjs": "7.5.5",
    "sass": "1.52.3",
    "scully-plugin-amp-css": "2.0.0",
    "ts-loader": "9.3.0",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-builders/jest": "14.0.0",
    "@angular-devkit/build-angular": "14.0.1",
    "@angular/cli": "14.0.1",
    "@angular/compiler-cli": "14.0.1",
    "@angular/language-service": "14.0.1",
    "@compodoc/compodoc": "1.1.19",
    "@nguniversal/builders": "14.0.0",
    "@types/bingmaps": "0.0.1",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.1",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.42",
    "@types/ramda": "0.28.14",
    "angular-cli-ghpages": "1.0.0",
    "codelyzer": "6.0.2",
    "eslint": "8.17.0",
    "express": "4.18.1",
    "jasmine-marbles": "0.9.2",
    "jest": "28.1.1",
    "jest-preset-angular": "12.1.0",
    "ng-packagr": "14.0.2",
    "prettier": "2.7.0",
    "protractor": "~7.0.0",
    "schematics-utilities": "2.0.3",
    "source-map-explorer": "2.5.2",
    "ts-node": "10.8.1",
    "tslint-angular": "3.0.3",
    "typescript": "4.7.3",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0"
  }
}