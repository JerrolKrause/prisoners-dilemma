{
  "name": "angular-starter",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "localize": "ren tsconfig.json tsconfig.old.json && ren tsconfig.bak.json tsconfig.json",
    "start": "npm run format && ng serve",
    "reinstall": "del package-lock.json /q && rmdir node_modules /s /q && npm i",
    "format": "npm run format-glob -- \"src/{app,environments}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format-glob": "prettier --write",
    "ng": "ng",
    "bit:pull": "bit import && bit checkout latest --all",
    "bit:push": "bit tag --all && bit export nts.angular",
    "bit:reset": "bit checkout --all --reset",
    "build": "ng build",
    "build:stats": "ng build --configuration=production --stats-json && npm run bundle-report",
    "build:source": "ng build --configuration=production --source-map",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json -p 8887",
    "prod": "http-server ./dist/browser -o -g",
    "build:qa": "ng build --configuration=qa",
    "build:uat": "ng build --configuration=uat",
    "build:prod": "ng build --configuration=production",
    "deploy": "ng build --prod --base-href /angular-starter/ && angular-cli-ghpages --no-silent --dir=dist/browser",
    "gzip": "gzipper --verbose ./dist/browser",
    "docs": "compodoc -p tsconfig.json",
    "docs:serve": "compodoc -p tsconfig.json -s",
    "docs:watch": "compodoc -p tsconfig.json -s -w",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-starter:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build-ie-polyfills": "webpack-cli src/polyfills-ie.ts -o src/assets/scripts/polyfills-ie.min.js -c webpack-polyfill.config.js"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 160,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "13.2.0",
    "@angular/animations": "13.2.0",
    "@angular/cdk": "13.2.0",
    "@angular/common": "13.2.0",
    "@angular/compiler": "13.2.0",
    "@angular/core": "13.2.0",
    "@angular/elements": "13.2.0",
    "@angular/forms": "13.2.0",
    "@angular/platform-browser": "13.2.0",
    "@angular/platform-browser-dynamic": "13.2.0",
    "@angular/platform-server": "13.2.0",
    "@angular/router": "13.2.0",
    "@angular/service-worker": "13.2.0",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-free": "6.1.0",
    "@fortawesome/fontawesome-svg-core": "~6.1.0",
    "@fortawesome/free-brands-svg-icons": "6.1.0",
    "@fortawesome/free-regular-svg-icons": "6.1.0",
    "@fortawesome/free-solid-svg-icons": "6.1.0",
    "@ngneat/until-destroy": "9.0.0",
    "@ntersol/schematics": "0.0.70",
    "@ntersol/state-management": "2.0.3",
    "@schematics/update": "0.1102.18",
    "bootstrap": "5.1.3",
    "core-js": "3.20.3",
    "document-register-element": "1.14.10",
    "helpful-decorators": "2.1.0",
    "primeicons": "5.0.0",
    "primeng": "13.3.0",
    "ramda": "0.28.0",
    "rxjs": "7.5.2",
    "sass": "1.49.0",
    "ts-loader": "9.2.6",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "13.0.3",
    "@angular-devkit/build-angular": "13.2.0",
    "@angular/cli": "13.2.0",
    "@angular/compiler-cli": "13.2.0",
    "@angular/language-service": "13.2.0",
    "@compodoc/compodoc": "1.1.18",
    "@types/bingmaps": "0.0.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.12",
    "@types/ramda": "0.27.40",
    "angular-cli-ghpages": "1.0.0",
    "codelyzer": "6.0.2",
    "eslint": "8.8.0",
    "express": "4.17.2",
    "jasmine-marbles": "0.8.4",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "ng-packagr": "13.2.0",
    "prettier": "2.5.1",
    "protractor": "~7.0.0",
    "schematics-utilities": "2.0.3",
    "source-map-explorer": "2.5.2",
    "ts-node": "10.4.0",
    "tslint-angular": "3.0.3",
    "typescript": "4.5.5",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2"
  }
}
